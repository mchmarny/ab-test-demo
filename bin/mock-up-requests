#!/bin/bash

u="https://signup-2gtouos2pq-uc.a.run.app/"
f=10

if [ "$1" == "b" ]; then
    f=2
fi


for i in $(seq 1 1000); do
    # always visit
    curl -s "${u}/?i=${i}" > /dev/null

    if ! (( $i % $f )) ; then
        # form by freqancy
        curl -s "${u}/form?i=${i}" > /dev/null
    fi

    sleep 0.5
done